{
  "name": "termite/admin",
  "description": "Termite CMS Admin plugin",
  "type":"cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "almasaeed2010/adminlte": "^2.3",
    "cakephp/cakephp": "^3.3.0",
    "wyrihaximus/twig-view": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "~1.5.6"
  },
  "autoload": {
    "psr-4": {
      "Termite\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Termite\\Tests\\Admin\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.0"
    },
    "sort-packages": true
  }
}
